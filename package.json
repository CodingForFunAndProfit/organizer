{
    "name": "organizer",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:local": "ng build --configuration=pwa --outputPath=dist",
        "build:prod": "ng build --prod --outputPath=docs",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test:gh": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCustom",
        "e2e:gh": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
        "build:ci": "npm run build:prod",
        "cov": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCustom --code-coverage",
        "serve": "http-server -p 4201 -c-1 dist"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.1",
        "@angular/cdk": "^9.2.0",
        "@angular/common": "~9.1.1",
        "@angular/compiler": "~9.1.1",
        "@angular/core": "~9.1.1",
        "@angular/flex-layout": "^9.0.0-beta.29",
        "@angular/forms": "~9.1.1",
        "@angular/material": "^9.2.0",
        "@angular/platform-browser": "~9.1.1",
        "@angular/platform-browser-dynamic": "~9.1.1",
        "@angular/router": "~9.1.1",
        "@angular/service-worker": "~9.1.1",
        "apollo-angular": "1.8.0",
        "apollo-angular-link-http": "1.9.0",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-client": "^2.6.8",
        "apollo-link-context": "^1.0.20",
        "graphql": "14.6.0",
        "graphql-tag": "^2.10.3",
        "ngx-cookie-service": "^3.0.4",
        "rxjs": "~6.5.5",
        "tslib": "^1.11.1",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.1",
        "@angular/cli": "~9.1.1",
        "@angular/compiler-cli": "~9.1.1",
        "@angular/language-service": "~9.1.1",
        "@types/jasmine": "~3.5.10",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "^13.11.1",
        "codelyzer": "^5.2.2",
        "http-server": "^0.12.1",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.1",
        "karma": "~6.3.14",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "~3.1.1",
        "karma-jasmine-html-reporter": "^1.5.3",
        "protractor": "~5.4.3",
        "rimraf": "^3.0.2",
        "ts-node": "~8.8.2",
        "tslint": "~6.1.1",
        "typescript": "3.7.5"
    }
}
